<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jasrsir.tracing">

    <application
        android:name=".adapter.Repository"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:logo="@id/imgLogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.LoginMain_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.SelectorUser_Activity" />
        <activity android:name=".activity.SignIn_Activity" />
        <activity android:name=".activity.SignUp_Activity" />
        <activity android:name=".activity.Wall_Activity" />
        <activity android:name=".activity.SignLost_Activity" />
        <activity android:name=".activity.About_Activity" />
        <activity android:name=".viewlistRecicler.ListView_Activity" />
        <activity android:name=".viewlistRecicler.RecyclerView_Activity" />
        <activity android:name=".activity.NewEvent_Activity" />
        <activity
            android:name=".activity.SettingsActivity"
            android:label="@string/title_activity_settings"></activity>
    </application>

</manifest>